            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.nexzenstudent.GrowthActivity"
            android:label="@string/title_activity_growth"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.nexzenstudent.HolidaysActivity"
            android:label="@string/title_activity_holidays"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.nexzenstudent.NewsActivity"
            android:label="@string/title_activity_news"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.nexzenstudent.NoticeActivity"
            android:label="@string/title_activity_notice"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.nexzenstudent.EnquiryActivity"
            android:label="@string/title_activity_enquiry"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.nexzenstudent.ResultListActivity"
            android:label="@string/title_activity_result_list"
