        <activity
            android:name="com.nexzenstudent.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name="com.nexzenstudent.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nexzenstudent.ResultActivity"
            android:label="@string/title_activity_result"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.nexzenstudent.ExamActivity"
            android:label="@string/title_activity_exam"
